  },
  "homepage": "https://www.assistant-ui.com/safe-content-frame",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/assistant-ui/assistant-ui.git",
    "directory": "packages/safe-content-frame"
  },
  "bugs": {
    "url": "https://github.com/assistant-ui/assistant-ui/issues"
